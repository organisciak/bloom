    {
      "name": "pixelate",
      "description": "Signature: <code>.pixelate( x, y )</code><br>Pixelate texture with <code>pixelX</code> segments and <code>pixelY</code> segments.",
      "params": [
        {
          "name": "pixelX",
          "type": {
            "names": [
              "float"
            ]
          },
          "description": "default 20.0"
        },
        {
          "name": "pixelY",
          "type": {
            "names": [
              "float"
            ]
          },
          "description": "default 20.0"
        }
      ],
      "examples": [
        "// default\nnoise().pixelate(20,20).out(o0)\n\nnoise()\n  .mult(osc(10,0.25,1))\n  .scrollY(1,0.25)\n  .pixelate([100,40,20,70].fast(0.25))\n  .modulateRotate(src(o0).scale(0.5),0.125)\n  .diff(src(o0).rotate([-0.05,0.05].fast(0.125)))\n  .out(o0)"
      ],
      "meta": {
        "source": "hydra",
        "category": "Geometry"
      }
    },
